#include <behaviors.dtsi>
#include <dt-bindings/zmk/bt.h>
#include <dt-bindings/zmk/keys.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
&kp ESCAPE        &kp Q         &kp W  &kp E  &kp R             &kp T                                   &kp Y            &kp U      &kp I      &kp O     &kp P              &kp MINUS
&kp TAB           &kp A         &kp S  &kp D  &kp F             &kp G                                   &kp H            &kp J      &kp K      &kp L     &kp SEMI           &kp SQT
&kp LEFT_CONTROL  &kp Z         &kp X  &kp C  &kp V             &kp B                                   &kp N            &kp M      &kp COMMA  &kp DOT   &mt FSLH UP_ARROW  &kp RSHFT
&kp LEFT_SHIFT    &kp LEFT_ALT  &none  &none  &kp LEFT_COMMAND  &lt 1 DELETE  &lt 2 SPACE  &lt 1 ENTER  &lt 2 BACKSPACE  &kp LANG1  &kp LANG3  &kp LEFT  &kp DOWN_ARROW     &kp RIGHT
            >;
        };

        lower_layer {
            bindings = <
&kp ESC    &kp N1    &kp N2  &kp N3    &kp N4      &kp N5                      &trans                &kp MINUS              &kp SLASH           &kp N7        &kp N8        &kp N9
&kp GRAVE  &kp EXCL  &kp AT  &kp HASH  &kp DOLLAR  &kp PRCNT                   &kp CARET             &kp PLUS               &kp ASTERISK        &kp NUMBER_4  &kp NUMBER_5  &kp NUMBER_6
&kp RET    &none     &none   &none     &none       &none                       &kp LEFT_PARENTHESIS  &kp RIGHT_PARENTHESIS  &kp KP_EQUAL_AS400  &kp NUMBER_1  &kp NUMBER_2  &kp NUMBER_3
&none      &none     &none   &trans    &trans      &trans     &kp DEL  &trans  &mo 3                 &trans                 &trans              &kp PERIOD    &kp NUMBER_0  &kp EQUAL
            >;
        };

        raise_layer {
            bindings = <
&kp F1  &kp F2  &kp F3  &kp F4  &kp F5  &kp F6                   &kp F7    &kp F8     &kp F9     &kp F10   &kp F11    &kp F12
&none   &none   &none   &none   &none   &none                    &none     &kp LEFT   &kp DOWN   &kp UP    &kp RIGHT  &none
&none   &none   &none   &none   &none   &none                    &kp PLUS  &kp MINUS  &kp EQUAL  &kp LBKT  &kp RBKT   &kp BSLH
&none   &none   &none   &trans  &trans  &mo 3   &trans  &kp DEL  &trans    &trans     &trans     &none     &none      &none
            >;
        };

        bt_layer {
            bindings = <
&bt BT_CLR      &bt BT_SEL 0  &bt BT_SEL 1  &bt BT_SEL 2  &bt BT_SEL 3  &bt BT_SEL 4                &none  &none  &none  &none  &none  &none
&studio_unlock  &none         &none         &none         &none         &none                       &none  &none  &none  &none  &none  &none
&none           &none         &none         &none         &none         &none                       &none  &none  &none  &none  &none  &none
&kp O           &kp M         &kp U         &none         &none         &none         &none  &none  &none  &none  &none  &kp K  &kp E  &kp N
            >;
        };
    };
};
